
@{
    ViewBag.Title = "DeleteConfirm";
}


<a class="returnlink" href="/Teacher/List">Back To Teachers</a>

<div id="show-container">
    
    <h2>Are you sure you want to delete @Model.TeacherFname @Model.TeacherLname ?</h2>
    
    <form action="/Teacher/Delete/@Model.TeacherId" method="POST">
        <input type="submit" value="Delete Teacher"/>
    </form>
   

</div>